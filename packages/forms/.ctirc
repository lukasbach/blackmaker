{
  addNewline: true,
  excludes: [
    '@types',
    'typings',
    '__test__',
    '__tests__',
    'node_modules',
    '.storybook',
    '.github',
    'temp',
    'out',
  ],
  fileExcludePatterns: [
    '*.stories.tsx',
    '*.storyhelper.tsx',
    '*.spec.tsx',
    '*.stories.ts',
    '*.storyhelper.ts',
    '*.spec.ts',
    '*.js',
  ],
  fileFirst: false,
  globOptions: {
    cwd: '.',
    dot: true,
    ignore: [
      '@types/**',
      'typings/**',
      '__test__/**',
      '__tests__/**',
      'node_modules/**',
    ],
    nonull: true,
  },
  includeCWD: true,
  output: 'index.ts',
  quote: "'",
  targetExts: [
    'ts',
    'tsx',
  ],
  useSemicolon: true,
  useTimestamp: false,
  verbose: false,
  withoutBackupFile: true,
  withoutComment: false,
}
