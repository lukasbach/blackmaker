{
  "name": "typed-react-package-starter",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:lukasbach/typed-react-package-starter.git"
  },
  "author": "Lukas Bach",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "lerna run start --parallel --npm-client=yarn",
    "build": "lerna run build --npm-client=yarn --stream",
    "test": "lerna run test --npm-client=yarn --stream",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.14",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.11",
    "@types/storybook__react": "^5.2.1",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "react-docgen": "^5.3.1",
    "react-docgen-typescript": "^1.20.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "storybook-addon-react-docgen": "^1.2.42",
    "ts-loader": "^8.0.11"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "storybook-dark-mode": "^1.0.4"
  }
}
