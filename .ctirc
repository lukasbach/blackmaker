{
  addNewline: true,
  excludes: [
    '@types',
    'typings',
    '__test__',
    '__tests__',
    'node_modules',
    '.storybook',
    '.github',
    'temp',
    'out',
  ],
  fileExcludePatterns: [],
  fileFirst: false,
  globOptions: {
    cwd: '.',
    dot: true,
    ignore: [
      '@types/**',
      'typings/**',
      '__test__/**',
      '__tests__/**',
      'node_modules/**',
      '*.stories.*',
      '*.storyhelper.*',
      '*.spec*',
      '*.js',
    ],
    nonull: true,
  },
  includeCWD: true,
  output: 'index.ts',
  quote: "'",
  targetExts: [
    'ts',
    'tsx',
  ],
  useSemicolon: true,
  useTimestamp: false,
  verbose: false,
  withoutBackupFile: false,
  withoutComment: false,
}
